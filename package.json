{
  "name": "apptestingcontext",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test-u": "jest -u",
    "test-watch": "jest --watchAll",
    "test-cover": "jest --collect-coverage"
  },
  "dependencies": {
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.4.1",
    "expo": "~44.0.0",
    "expo-status-bar": "~1.2.0",
    "jest": "^27.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1",
    "react-test-renderer": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/**/*.(spec|test).(js)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/ios",
      "<rootDir>/android",
      "<rootDir>/node_modules",
      "<rootDir>/__tests__/jest.setup.js"
    ]
  }
}
